{
  "name": "api-engine",
  "version": "1.0.0",
  "description": "api-engine",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f ./config/dev.env cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/index.ts",
    "postinstall": "npm run codegen && npm run generate",
    "migrate": "env-cmd -f ./config/dev.env npx prisma migrate dev",
    "db-push": "env-cmd -f ./config/dev.env npx prisma db push",
    "generate": "npx prisma generate",
    "seed": "env-cmd -f ./config/dev.env prisma db seed",
    "studio": "env-cmd -f ./config/dev.env npx prisma studio",
    "codegen": "graphql-codegen --config codegen.yaml",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --config .prettierrc --check .",
    "prettier:fix": "prettier --config .prettierrc --write .",
    "test": "env-cmd -f ./config/test.env jest --watchAll --detectOpenHandles --runInBand --testTimeout=12000",
    "clean": "rimraf coverage dist tmp",
    "check": "npm run lint && npm run codegen",
    "prebuild": "npm run check",
    "prebuild:release": "npm run check",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.json",
    "build:release": "npm run clean && cross-env NODE_ENV=production env-cmd -f ./config/prod.env tsc -p tsconfig.release.json",
    "start:prod": "cross-env NODE_ENV=production env-cmd -f ./config/prod.env node dist/index.js",
    "migrate:deploy": "env-cmd -f ./config/prod.env npx prisma migrate deploy",
    "db-push:prod": "env-cmd -f ./config/prod.env npx prisma db push"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/subgraph": "^2.9.3",
    "@paljs/plugins": "^7.0.2",
    "@prisma/client": "^5.22.0",
    "@whatwg-node/fetch": "^0.9.21",
    "bcryptjs": "^2.4.3",
    "express": "^4.21.0",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.9.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.11.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.11.1",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.0",
    "@graphql-codegen/typescript-resolvers": "^4.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.5",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "casual": "^1.6.2",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-mock-extended": "2.0.4",
    "prettier": "3.3.3",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.6.0"
  },
  "overrides": {
    "eslint": "^9.11.1"
  },
  "volta": {
    "node": "20.18.0",
    "pnpm": "9.12.3"
  }
}
